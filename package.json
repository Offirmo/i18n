{
  "name": "@offirmo/internationalize",
  "version": "0.0.1",
  "description": "A powerful, standard-compliant i18n lib for your JS / TS app, built around yahoo's excellent FormatJS lib.",
  "main": "index.js",
  "scripts": {
    "__prod": "~~~~~~~ PRODUCTION ~~~~~~~",
    "test": "mocha --opts test/unit/mocha.opts node_modules/@offirmo/unit-test-toolbox/mocha-chai-init-node.js test/unit/init.js 'test/unit/src/**/*_spec.js'",
    "build": "node scripts/build.js",
    "np": "np",
    "__dev": "~~~~~~~ DEVELOPMENT ~~~~~~~",
    "clean": "rm -rf test/unit/src",
    "dev1": "node scripts/build-for-unit-tests.js",
    "dev2": "cpx 'src/**/*.json' test/unit/src --watch",
    "dev": "run-p dev1 dev2",
    "__misc": "~~~~~~~ MISC ~~~~~~~",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Offirmo/internationalize.git"
  },
  "keywords": [
    "i18n",
    "internationalization"
  ],
  "author": "Offirmo <offirmo.net@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Offirmo/internationalize/issues"
  },
  "homepage": "https://github.com/Offirmo/internationalize#readme",
  "dependencies": {
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "intl-messageformat": "^1.3.0",
    "intl-relativeformat": "^1.3.0",
    "lodash": "^4.16.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "@offirmo/unit-test-toolbox": "1.0.0",
    "@types/lodash": "^4.14.30",
    "@types/node": "^6.0.41",
    "@types/underscore.string": "^0.0.30",
    "babel-preset-es2015-node-rollup": "^6.1.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "counterpart": "^0.17.5",
    "cpx": "^1.3.2",
    "find-glob": "^2.1.0",
    "fs-extra": "^0.30.0",
    "loud-rejection": "^1.6.0",
    "mocha": "^3.0.2",
    "node-typescript-compiler": "^1.0.1",
    "nodemon": "^1.10.0",
    "np": "^2.6.0",
    "npm-run-all": "^3.0.0",
    "pre-commit": "^1.1.3",
    "rollup": "^0.36.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sinon": "^1.17.4",
    "ts-node": "^1.3.0",
    "tslint": "^3.14.0",
    "typescript": "^2.0.2"
  }
}
